					<version>3.0</version>
					<configuration>
						<source>${java.source.version}</source>
						<target>${java.source.version}</target>
						<showWarnings>true</showWarnings>
					</configuration>
				</plugin>

				<!-- clean -->
				<plugin>
					<artifactId>maven-clean-plugin</artifactId>
					<version>2.5</version>
